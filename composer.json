{
    "name": "sudiptpa/preferred-domain",
    "description": "Set Preferred Domain for Laravel Application with a Middleware.",
    "keywords": ["canonical domain", "preferred domain"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sujip Thapa",
            "email": "sudiptpa@gmail.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/http" : "~5.5.0",
        "illuminate/support": "~5.5.0"
    },
    "require-dev": {
      "friendsofphp/php-cs-fixer":"~2.0",
      "orchestra/testbench": "^3.5",
      "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Sujip\\Middleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sujip\\Middleware\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Sujip\\PreferredDomainServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
