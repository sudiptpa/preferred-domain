{
    "name": "sudiptpa/preferred-domain",
    "description": "Set preferred domain for Laravel application with a middleware.",
    "keywords": ["canonical domain", "preferred domain"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sujip Thapa",
            "email": "sudiptpa@gmail.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/contracts": "~5.4.0|~5.5.0",
        "illuminate/support": "~5.4.0|~5.5.0",
    },
    "require-dev": {
      "friendsofphp/php-cs-fixer":"~2.0",
      "mockery/mockery": "^0.9.4",
      "phpunit/phpunit": "6.*"
    },
    "autoload": {
        "psr-4": {
            "Sujip\\PreferredDomain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sujip\\PreferredDomain\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Sujip\\PreferredDomainServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
