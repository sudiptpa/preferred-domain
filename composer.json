{
    "name": "sudiptpa/domain",
    "description": "Set preferred domain for Laravel application.",
    "keywords": ["SEO", "preferred domain"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sujip Thapa",
            "email": "sudiptpa@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/contracts": "~5.0"
    },
    "require-dev": {
        "illuminate/support": "~5.0",
        "mockery/mockery": "~0.9.3",
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer":"~1.0"
    },
    "autoload": {
        "psr-4": {
            "Sujip\\Domain\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Sujip\\Domain\\PreferredDomainServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
